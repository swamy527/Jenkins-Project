pipeline {
    agent {
    node {
        label 'Agent-1'
         }
    }

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('How') {
            steps {
                echo 'Here how stage executed'
            }
        }
        stage('where') {
            steps {
                echo 'Here where stage executed'
            }
        }
        stage('when') {
            steps {
                echo 'Here when stage executed'
            }
        }
    }
}
